                                  <th>Total</th>\n                                      <th>Status</th>\n                                      <th>Action</th>\n                                    </tr>\n                                   </thead>\n                                 <tbody>\n\n                                    <tr v-for=\"(order,index) in orders\" :key=\"index\">\n                                       <td>{{index+1}}</td>\n                                       <td>{{order.customer}}</td>\n                                       <td>{{order.nitems}}</td>\n                                       <td>{{order.price*order.nitems}}</td>\n                                       <td>{{order.status}}</td>\n                                       <td>\n                                           <button type=\"button\" class=\"btn btn-primary\" @click=\"changeStatus(order.id)\">Primaryy</button>\n                                         \n                                       </td>\n                                    </tr>\n                                 </tbody>\n                              </table>\n                        </div> \n                     </div>\n                  </div>\n                  <!--display table ends -->\n  </div>\n</template>\n\n<script>\n import HelloWorld from './components/HelloWorld.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    HelloWorld\n  },\n  data(){\n    return{\n        received_orders:[\n          {id:1,customer:\"sanju\",nitems:3,price:30,status:\"received\"},\n          {id:2,customer:\"suresh\",nitems:6,price:50,status:\"received\"},\n          {id:3,customer:\"manu\",nitems:4,price:10,status:\"received\"}\n        ],\n       orders:[],\n    }\n  },\n  methods:{\n     changeStatus(id){\n          this.orders.map((el)=>{\n             if((el.id==id)&&(el.status=='received')){\n                 el.status='prepared';\n             }else if((el.id==id)&&(el.status=='prepared')){\n                 el.status='ready';\n             }\n           })\n          localStorage.setItem('orders',JSON.stringify(this.orders));\n       },\n  },\n  mounted() {\n      if(localStorage.getItem('orders')==null){\n          this.orders = this.received_orders;\n          localStorage.setItem('orders',JSON.stringify(this.received_orders));\n      }else{\n          this.orders = JSON.parse(localStorage.getItem(\"orders\"));\n      }\n  },\n}\n</script>\n\n<style>\n/* #app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n} */\n  @import './assets/styles.css';\n</style>\n"]}]}